<body class="container">
    <h3 class="header">WebScraper</h3>
    <div id="buttons">
        <button class="btn" id="headline-sort">Scrape</button>
    </div>
    <div class="row">
        <div class="col s2"></div>
        <div class="col s8 left-align">
            {{#each article}}
            <div class="card-panel">
                <h4>{{this.title}}</h4>
                <a href="/readArticle/{{this.id}}" class="waves-effect waves-light btn" id="btnSignUp">Read Article</a>
            </div>
        </div>
    </div>